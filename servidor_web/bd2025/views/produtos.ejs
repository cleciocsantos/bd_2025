<!DOCTYPE html>
<html>
<head>
  <title><%= title %></title>
  <style>
    body { font-family: Arial; margin: 30px; }
    table { width: 100%; border-collapse: collapse; }
    th, td { padding: 8px; border: 1px solid #ccc; text-align: left; }
    th { background: #eee; }
  </style>
</head>
<body>
  <h1><%= title %></h1>
  <table>
    <thead>
      <tr>
        <th>ID</th>
        <th>Nome</th>
        <th>Pre√ßo</th>
        <th>Categoria</th>
      </tr>
    </thead>
    <tbody>
    <% items.forEach(function(item) { %>
        <tr>
          <td><%= item.id %></td>
          <td><%= item.nome %></td>
          <td><%= item.preco.toFixed(2) %></td>
          <td><%= item.categoria %></td>
        </tr>
    <% }); %>
    </tbody>
  </table>
</body>
</html>